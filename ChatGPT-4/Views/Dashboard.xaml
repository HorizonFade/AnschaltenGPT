<Page x:Class="ChatGPT_4.Views.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d"
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Dashboard">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Dies ist ein kleines Programm um ChatGPT zu nutzen" HorizontalAlignment="Center"
                   VerticalAlignment="Bottom" Foreground="White" FontFamily="Cascadia Code" FontSize="18" />
        <TextBlock Grid.Row="1" Text="v1.0" HorizontalAlignment="Left"
                   VerticalAlignment="Bottom" Foreground="White" FontFamily="Cascadia Code" FontSize="12" Margin="10,0,0,0"/>
        <TextBlock Grid.Row="1" Text="Made by Anschalten" HorizontalAlignment="Right"
                   VerticalAlignment="Bottom" Foreground="White" FontFamily="Cascadia Code" FontSize="12" Margin="0,10,10,0"/>
    </Grid>
</Page>